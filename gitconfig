[user]
	name = Brent Yorgey
	email = byorgey@gmail.com
[merge]
	tool = vimdiff
[core]
	editor = emacs
[alias]
	co = checkout
	ci = commit
	br = branch
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
	ix = diff --cached
        pulled = log -p --reverse --no-merges --stat @{1}..
        url =! bash -c 'git config --get remote.origin.url | sed -E "s/.+:\\(.+\\)\\.git$/https:\\\\/\\\\/github\\\\.com\\\\/\\\\1/g"'
[push]
	default = simple
